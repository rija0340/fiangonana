<div class=" pt-3 pb-3 text-center">
	<h3>Formulaire</h3>
	<p>
		Entrer un nom de famille
	</p>
</div>
{{form_start(form,  {'action': path('filadelfia_creer_famille')})}}
<label for="">Nom de famille</label>
{{form_widget(form.nom)}}
<label for="">Membre de cette famille (optionnel)</label>
{{form_widget(form.mambra)}}
<label for="">Sexe de la membre de famille
</label>
{{form_widget(form.sexe)}}
<button class="btn btn-danger mt-2">
	Enregistrer</button>
{{form_end(form)}}
