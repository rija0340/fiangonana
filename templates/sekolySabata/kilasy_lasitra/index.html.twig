{% extends 'baseAdminLTE.html.twig' %}
{% block title %}Filadelfia
{% endblock %}
{% block stylesheets %}{% endblock %}
{% block title_page %}Kilasy lasitra
{% endblock %}
{% block current_page %}liste
{% endblock %}

{% block content %}

	<div class="card">
		<div class="card-header">
			<div class="d-flex">
				<div class="mr-auto p-2 bd-highlight">
					<h5 class="title">Kilasy lasitra
					</h5>
				</div>
				<div class="row">
					<div class="mr-2">
						<a class="btn btn-outline-primary" href="{{ path('kilasy_lasitra_new') }}">Nouvelle kilasy</a>
					</div>
				</div>
			</div>
		</div>
		<!-- /.card-header -->
		<div class="card-body">
			<table id="example1" class="table table-bordered table-striped">
				<thead>
					<tr>
						<th>Id</th>
						<th>Nom</th>
						<th>TrancheAge</th>
						<th>Description</th>
						<th>actions</th>
					</tr>
				</thead>
				<tbody>
					{% for kilasy_lasitra in kilasy_lasitras %}
						<tr>
							<td>{{ kilasy_lasitra.id }}</td>
							<td>{{ kilasy_lasitra.nom }}</td>
							<td>{{ kilasy_lasitra.trancheAge }}</td>
							<td>{{ kilasy_lasitra.description }}</td>
							<td>
								<div
									class="">
									<!-- Check all button -->
									<div class="btn-group">
										<a href="{{ path('kilasy_lasitra_show', {'id': kilasy_lasitra.id}) }}" type="button" class="btn btn-default btn-sm" title="Voir">
											<i class="fas fa-eye"></i>
										</a>

										<a href="{{ path('kilasy_lasitra_edit', {'id': kilasy_lasitra.id}) }}" type="button" class="btn btn-default btn-sm" title="Modifier">
											<i class="fas fa-edit"></i>
										</a>

										{{ include('sekolySabata/kilasy_lasitra/_delete_form.html.twig') }}
									</div>
									<!-- /.btn-group -->

								</div>
							</td>
						</tr>
					{% endfor %}
				</tbody>
			</table>

		</div>
		<!-- /.card-body -->
	</div>

{% endblock %}
